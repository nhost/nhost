    "@types/prettier": "^2.6.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3"
