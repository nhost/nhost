    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^5.37.2",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^0.25.8"
